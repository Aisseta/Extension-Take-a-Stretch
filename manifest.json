{
    "manifest_version": 3,
    "name": "Take A Strech",
    "version": "1.0",
    "description": "Prenez une pause au bureau avec nos exercices de posture !",
    "icons": {
      "1": "icone.png"
   
    },
    "action": {
      "default_icon": "icone.png",
      "default_title": "Take A Stretch",
      "default_popup": "popup.html"
    },
    "permissions": ["storage", "alarms", "notifications"],
    "background": {
      "service_worker": "background.js"
    }
  }