{
    "manifest_version": 3,
    "name": "Prend une pause",
    "version": "1.0",
    "description": "Etirement au bureau",
    "icons": {
      "1": "icon.png"
   
    },
    "action": {
      "default_icon": "icon.png",
      "default_title": "BREAK TIME",
      "default_popup": "popup.html"
    },
    "permissions": ["storage", "alarms", "notifications"],
    "background": {
      "service_worker": "background.js"
    }
  }